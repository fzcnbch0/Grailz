generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model jednostki {
  id        Int     @id @default(autoincrement()) @db.UnsignedInt
  nazwaJedn String? @db.VarChar(20)
}

model materialy {
  id            Int     @id @default(autoincrement()) @db.UnsignedInt
  Producenci_id Int     @db.UnsignedInt
  Jednostki_id  Int     @db.UnsignedInt
  nazwa         String? @db.Text
  ilosc         Int?    @db.UnsignedInt
}

model producenci {
  id        Int     @id @default(autoincrement()) @db.UnsignedInt
  nazwaProd String? @db.VarChar(50)
}
